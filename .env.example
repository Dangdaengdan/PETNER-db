# PETNER Database Environment Configuration
# Copy this file to .env and fill in your values

# PostgreSQL Settings
POSTGRES_PORT=5432
POSTGRES_USER=petner
POSTGRES_PASSWORD=your_secure_postgres_password_here
POSTGRES_DB=petnerdb

# Redis Session Settings
REDIS_SESSION_PORT=6379

# Redis Cache Settings  
REDIS_CACHE_PORT=6380

# Optional: Redis Passwords (recommended for production)
# REDIS_SESSION_PASSWORD=your_session_redis_password
# REDIS_CACHE_PASSWORD=your_cache_redis_password